{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": [
        "plugin:angular/johnpapa"
    ],
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": "off",
        "no-debugger": "warn",
        "angular/di": [
            "error",
            "$inject"
        ]
    }
}